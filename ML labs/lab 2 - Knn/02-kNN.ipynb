      "     |  >>> lb.classes_\n",
      "     |  array([0, 1, 2])\n",
      "     |  >>> lb.transform([0, 1, 2, 1])\n",
      "     |  array([[1, 0, 0],\n",
      "     |         [0, 1, 0],\n",
      "     |         [0, 0, 1],\n",
